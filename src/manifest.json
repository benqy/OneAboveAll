{
  "name": "One Above All",
  "description": "根据需要自动插入脚本到页面,用于改造页面",
  "version": "1.0",
  "content_scripts": [
    {
	  "matches":["http://*/*","https://*/*"],
      "js": ["jquery-2.1.1.min.js","content.js"]
    }
  ],
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "background": { "scripts": ["background.js"] },
  "browser_action": {
      "default_title": "插入自定义脚本.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
